import axios from 'axios';

// URL de l'API d'inscription
const API_URL = 'http://localhost:8088/patients/{id}';   

export const signUp = async (userData: {
  phone: number;
  adress: string;
  BirthDate : Date;
  insuranceNumber: integer;
  insurancinceName: string;
 cin : nulber
}) => {
  try {
    // Envoi des données via une requête POST
    const response = await axios.put(API_URL, {
       phone: number;
  adress: string;
  BirthDate : Date;
  insuranceNumber: integer;
  insurancinceName: string;
 cin : nulber
    });
    return response.data; 
  } catch (error:any) {
    // Si une erreur se produit, on la capture et la renvoie
    throw error.response ? error.response.data : error.message;
  }
};
